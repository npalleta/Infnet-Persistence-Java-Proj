<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="br.edu.infnet.project.db.model.domain.SalaAula" table="SalaAula">
        <id name="idSalaAula" type="integer">
            <column name="IdSalaAula"/>
            <generator class="identity"/>
        </id>
        <property name="idAluno" column="IdAluno" type="integer"/>
        <property name="idProfessor" column="IdProfessor" type="integer"/>
    </class>
</hibernate-mapping>